<?xml version="1.0" encoding="utf-8"?>
<configuration>

  <appSettings>
    <add key="ENV_BLAISE_SERVER_HOST_NAME" value="#{ENV_BLAISE_SERVER_HOST_NAME}#" />
    <add key="ENV_BLAISE_ADMIN_USER" value="#{ENV_BLAISE_ADMIN_USER}#" />
    <add key="ENV_BLAISE_ADMIN_PASSWORD" value="#{ENV_BLAISE_ADMIN_PASSWORD}#" />
    <add key="ENV_BLAISE_SERVER_BINDING" value="#{ENV_BLAISE_SERVER_BINDING}#" />
    <add key="ENV_BLAISE_CONNECTION_PORT" value="#{ENV_BLAISE_CONNECTION_PORT}#" />
    <add key="ENV_BLAISE_REMOTE_CONNECTION_PORT" value="#{ENV_BLAISE_REMOTE_CONNECTION_PORT}#" />
    <add key="ENV_CONNECTION_EXPIRES_IN_MINUTES" value="#{ENV_CONNECTION_EXPIRES_IN_MINUTES}#" />
    <add key="ENV_DB_CONNECTIONSTRING" value="#{ENV_DB_CONNECTIONSTRING}#" />
    <add key="ENV_BLAISE_DQS_BUCKET" value="#{ENV_BLAISE_DQS_BUCKET}#" />
    <add key="ENV_BLAISE_NISRA_BUCKET" value="#{ENV_BLAISE_NISRA_BUCKET}#" />
    <add key="ENV_BLAISE_INGEST_BUCKET" value="#{ENV_BLAISE_INGEST_BUCKET}#" />
    <add key="ENV_RESTAPI_PORT" value="#{ENV_RESTAPI_PORT}#" />
    <add key="BASE_URL" value="http://*:" />
    <add key="TEMP_PATH" value="#{TEMP_PATH}#" />
    <add key="PACKAGE_EXTENSION" value="#{PACKAGE_EXTENSION}#" />
    <add key="ClientSettingsProvider.ServiceUri" value="" />
    <add key="COMMAND_TIMEOUT" value="300" />
  </appSettings>

  <system.data>
    <DbProviderFactories>
      <add name="MySQL Data Provider"
           invariant="MySql.Data.MySqlClient"
           description=".Net Framework Data Provider for MySQL"
           type="MySql.Data.MySqlClient.MySqlClientFactory, MySql.Data, Version=9.4.0.0, Culture=neutral, PublicKeyToken=c5687fc88969c44d" />
      <add name="SQLite Data Provider"
           invariant="System.Data.SQLite"
           description=".Net Framework Data Provider for SQLite"
           type="System.Data.SQLite.SQLiteFactory, System.Data.SQLite, Version=1.0.119.0, Culture=neutral, PublicKeyToken=db937bc2d44ff139" />
    </DbProviderFactories>
  </system.data>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
  </startup>
  
</configuration>
